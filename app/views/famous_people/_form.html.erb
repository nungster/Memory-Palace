<%= simple_form_for(@famouspeople) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :code %>
    <%= f.input :title %>
    <%= f.input :first_name %>
    <%= f.input :middle_name %>
    <%= f.input :last_name %>
    <%= f.input :link %>
    <%= f.input :note %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
